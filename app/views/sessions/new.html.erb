<%= form_tag sessions_path do %>
<div class="usercontainer">
  <br>
  <div class="field">
    <%= label_tag :email %>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <br>
  <input type="submit" id="userlogin" value="Log In">
  </div>
</div>
<% end %>
